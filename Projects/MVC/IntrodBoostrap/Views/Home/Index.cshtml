<h5>Ejemplo 25: Toasts</h5>
<button onclick="mostrarToast()" class="btn btn-primary">Mostrar toast</button>

<div class="position-fixed bottom-0 end-0 p-3" style="z-index: 11">
    <div id="miToast" class="toast">
        <div class="toast-header">
            <strong class="me-auto">Mensaje enviado</strong>
            <small>11 mins ago</small>
            <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
        </div>
        <div class="toast-body">
            Mensaje fue enviado a Claudia
        </div>
    </div>
</div>


@section Scripts{
    <script>
        function mostrarToast(){
            let ejemploToast = document.getElementById('miToast');
            let toast = new bootstrap.Toast(ejemploToast);

            toast.show();
        }


        function mostrarAlertaExitosa(){
            mostrarAlerta('¡Exitoso!', 'success');
        }
        function mostrarAlertaError(){
            mostrarAlerta('¡Ocurrio un error!', 'danger');
        }

        function mostrarAlerta(message, type) {
            const alert = document.createElement("div");
            alert.innerHTML = `<div class="alert alert-${type}" role="alert">
            ${message}
            </div>`

            const contenedorAlert = document.getElementById("contenedor-alertas");
            contenedorAlert.append(alert);
        }
    </script>
}